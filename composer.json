    "homepage": "http://github.com/itakademy/monolog-logdna",
    "type": "library",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "Nicolas Vanheuverzwijn",
            "email": "nicolas.vanheu@gmail.com"
        },
